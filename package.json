        "directory": "next-graphql-blog",
        "url": "https://github.com/M0hammedImran/next-graphql-blog"
    },
    "keywords": [
        "nextjs",
        "blog",
        "react",
        "typescript"
    ],
    "private": true,
    "scripts": {
        "clean": "rm -rf node_modules",
        "dev": "next dev",
        "prebuild": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' --transpile-only src/graphql/schema",
        "build": "next build",
        "start": "next start",
        "prisma:migrate": "prisma migrate dev --preview-feature",
        "format": "prettier --write '**/*.@(js|ts|tsx|json|md|yml)'"
    },
    "dependencies": {
        "@prisma/client": "^2.18.0",
        "@tailwindcss/jit": "^0.1.3",
        "@tailwindcss/typography": "^0.4.0",
        "apollo-server-micro": "^2.21.0",
        "axios": "^0.21.1",
        "bcryptjs": "^2.4.3",
        "dataloader": "^2.0.0",
        "dayjs": "^1.10.4",
        "graphql": "^15.5.0",
        "micro-cors": "^0.1.1",
        "next": "10.0.7",
        "nexus": "^1.0.0",
        "nexus-plugin-prisma": "^0.32.0",
        "react": "17.0.1",
        "react-dom": "17.0.1",
        "ts-node": "^9.1.1"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.2",
        "@types/node": "^14.14.34",
        "@types/react": "^17.0.2",
        "autoprefixer": "^10.2.5",
