        "prettier": "^2.2.1",
        "prisma": "^2.18.0",
        "tailwindcss": "^2.0.3",
        "typescript": "^4.2.2"
    },
    "engines": {
        "node": ">=14.10.0"
    },
    "prettier": {
        "semi": true,
        "trailingComma": "all",
        "singleQuote": true,
        "printWidth": 120,
        "tabWidth": 4
    }
